<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>users</servlet-name>
        <servlet-class>SERVLETS.users</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>SERVLETS.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>preguntas</servlet-name>
        <servlet-class>SERVLETS.preguntas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>resultados</servlet-name>
        <servlet-class>SERVLETS.resultados</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>puntajes</servlet-name>
        <servlet-class>SERVLETS.puntajes</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>users</servlet-name>
        <url-pattern>/users</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>preguntas</servlet-name>
        <url-pattern>/preguntas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>resultados</servlet-name>
        <url-pattern>/resultados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>puntajes</servlet-name>
        <url-pattern>/puntajes</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
